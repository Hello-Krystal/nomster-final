<!-- The following simple form allows us to add a new place to app -->
<div class="booyah-box col-10 offset-1">
    <h1>Enter Your Place Details</h1>
    <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>

    <br />
    <!-- bootstrap feature to style button -->
    <%= f.submit 'Create', class: 'btn btn-primary' %>
    <% end %>
</div>